{
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "clean": "shx rm -rf chaindata",
    "start": "node -r dotenv/config index.js",
    "start:watch": "nodemon -r dotenv/config -w *.js -w .env index.js",
    "lint": "yarn run eslint .",
    "lint:fix": "yarn run eslint . --fix",
    "pretest": "shx rm -rf chaindata-test",
    "test": "cross-env BLOCKCHAIN_DATA_STORAGE_DIRECTORY=chaindata-test jest",
    "test:watch": "nodemon -w *.js --exec yarn test"
  },
  "name": "private-blockchain",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "level": "^4.0.0"
  }
}
